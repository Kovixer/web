[MASTER]
jobs=2

[MESSAGES CONTROL]
disable=unsubscriptable-object,unused-argument,no-name-in-module,duplicate-code

[FORMAT]
max-line-length=80

[MISCELLANEOUS]
notes=FIXME,BUG # TODO,UGLY

[BASIC]
good-names=i,j,e,f,by,ip,id,to,rm,el,tg,vk
no-docstring-rgx=[(__.*__)(Type)]

[DESIGN]
max-args=8
max-attributes=10
min-public-methods=0
